{
    "name": "lingoda/braze",
    "description": "Lingoda Braze Bundle",
    "type": "symfony-bundle",
    "version": "1.0",
    "require-dev": {
        "phpspec/phpspec": "^7.1.x-dev",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3"
    },
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "doctrine/annotations": "^1.13",
        "nesbot/carbon": "^2.48",
        "symfony/framework-bundle": "^4.4 || ^5.2",
        "symfony/http-client": "^5.2",
        "symfony/options-resolver": "^4.4 || ^5.2",
        "symfony/property-access": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/validator": "^4.4 || ^5.2",
        "webmozart/assert": "^1.9"
    },
    "conflict": {
        "psr/cache": ">=2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Lingoda\\BrazeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lingoda\\BrazeBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
